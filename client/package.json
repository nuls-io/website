{
  "name": "Nuls Resources Client",
  "version": "0.1.0",
  "description": "Nuls resource community driven project",
  "author": "Josh Welham",
  "license": "MIT",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --config build/webpack.prod.js",
    "dev": "node build/server.js",
    "app": "cross-env NODE_ENV=development electron app/",
    "test": "npm run lint && npm run unit",
    "lint": "./node_modules/.bin/eslint --ext js,vue build/ src/ test/",
    "lintDebug": "./node_modules/.bin/eslint --ext js,vue build/utils.js",
    "unit": "jest --no-watchman",
    "unitDebugger": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "babel": {
    "plugins": [
      "syntax-dynamic-import",
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "@": "./src",
            "__testConfig": "./testConfig"
          }
        }
      ]
    ],
    "presets": [
      [
        "env"
      ],
      "stage-2",
      [
        "vue-app",
        {
          "useBuiltIns": true
        }
      ]
    ],
    "env": {
      "test": {
        "presets": [
          "env",
          "stage-2"
        ],
        "plugins": []
      }
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-nested": {},
      "postcss-custom-media": {},
      "postcss-import": {},
      "postcss-custom-properties": {},
      "postcss-color-function": {}
    }
  },
  "browserslist": [
    "last 2 versions",
    "ie > 8"
  ],
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.spec.js?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    },
    "mapCoverage": true,
    "setupFiles": [
      "./testConfig/jestSetup.js"
    ]
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.18.0",
    "clone": "^2.1.1",
    "deepmerge": "^2.0.1",
    "dot-object": "^1.7.0",
    "flexboxgrid": "^6.3.1",
    "jest-webpack-alias": "^3.3.3",
    "lodash": "^4.17.4",
    "moment": "^2.20.1",
    "object-hash": "^1.2.0",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.2.0",
    "postcss-import": "^11.0.0",
    "promise-polyfill": "^6.1.0",
    "socket.io-client": "^2.0.4",
    "spin": "0.0.1",
    "traverse": "^0.6.6",
    "urijs": "^1.19.0",
    "uuid": "^3.1.0",
    "vee-validate": "^2.0.0-rc.25",
    "vue": "^2.5.9",
    "vue-fraction-grid": "^1.1.0",
    "vue-i18n": "7.3.2",
    "vue-masonry-css": "^1.0.0",
    "vue-multiselect": "^2.0.8",
    "vue-progressbar": "^0.7.3",
    "vue-router": "^3.0.1",
    "vue-spinner": "^1.0.3",
    "vue-tabs-component": "^1.4.0",
    "vue2-google-maps": "^0.8.10",
    "vuedraggable": "^2.15.0",
    "vuejs-datepicker": "^0.9.26",
    "vuetify": "^1.0.11",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.16",
    "@storybook/addon-links": "^3.2.16",
    "@storybook/vue": "^3.2.16",
    "autoprefixer": "^7.1.6",
    "babel": "^6.23.0",
    "babel-core": "^6.16.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-vue-app": "^1.3.2",
    "chalk": "^2.3.0",
    "copy-webpack-plugin": "^4.2.3",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.12.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.2.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.3.2",
    "eslint-plugin-vue": "^3.14.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.1.2",
    "html-webpack-plugin": "^2.22.0",
    "jest": "^21.2.1",
    "jest-vue-preprocessor": "^1.3.1",
    "moxios": "^0.4.0",
    "offline-plugin": "^4.8.4",
    "postcss-color-function": "^4.0.1",
    "postcss-loader": "^2.0.9",
    "postcss-nested": "^2.1.2",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.19.0",
    "vue-loader": "^13.5.0",
    "vue-server-renderer": "^2.5.9",
    "vue-template-compiler": "^2.5.9",
    "vue-test-utils": "^1.0.0-beta.6",
    "wallaby-vue-compiler": "^1.0.2",
    "webpack": "3.8.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-server": "^2.9.5"
  }
}
