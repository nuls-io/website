{
  "name": "NulsResroucesServer",
  "version": "0.0.1",
  "description": "Server for the commnuity driven Nuls Resources website",
  "main": "app.js",
  "author": "Josh Welham",
  "license": "MIT",
  "engines": {
    "node": ">=8.4.0",
    "npm": ">=5.0.0"
  },
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon --trace-warnings --config nodemon.json ./bin/www",
    "lint": "./node_modules/.bin/eslint --ext js,vue app.js data/ src/",
    "lintDebug": "./node_modules/.bin/eslint --ext js,vue build/utils.js",
    "unit": "jest --no-watchman",
    "unitDebugger": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "getNulsPrice": "node ./src/scripts/coinmarketcap/getNulsPrice.js"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.spec.js?(x)"
    ],
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "transform": {
      ".*\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "mapCoverage": true
  },
  "_moduleAliases": {
    "@": "./src",
    "__data__": "./data",
    "__root__": "./"
  },
  "babel": {
    "plugins": [
      "transform-runtime",
      "dynamic-import-node"
    ],
    "presets": [
      [
        "env",
        "es2015"
      ]
    ],
    "env": {
      "test": {
        "presets": [
          "env"
        ],
        "plugins": [
          "dynamic-import-node",
          [
            "module-resolver",
            {
              "root": [
                "./src"
              ],
              "alias": {
                "@": "./src",
                "__data__": "./data",
                "__root__": "./"
              }
            }
          ]
        ]
      }
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "bodybuilder": "^2.2.6",
    "chalk": "^2.3.0",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "deepmerge": "^2.0.1",
    "dot-object": "^1.7.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "express-unless": "^0.5.0",
    "jsonfile": "^4.0.0",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.5",
    "module-alias": "^2.0.2",
    "morgan": "^1.9.0",
    "mysql2": "^1.5.3",
    "nodemon": "^1.12.1",
    "raven": "^2.2.1",
    "simple-encryptor": "^1.1.1",
    "socket.io": "^2.0.4",
    "validator": "^9.1.2"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.16.0",
    "babel-eslint": "^8.0.2",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-dynamic-import-node": "^1.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.10.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.2.0",
    "jest": "^22.4.3"
  }
}
