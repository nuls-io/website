{
  "connection": "default",
  "collectionName": "dapps",
  "info": {
    "name": "dapps",
    "description": "Decentralised applications that run on NULS"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
    "active": {
      "type": "boolean"
    },
    "serialNumber": {
      "unique": true,
      "type": "integer"
    },
		"link": {
			"type": "string"
		},
		"image": {
			"model": "file",
			"via": "related",
			"plugin": "upload"
		},
    "en_title": {
      "type": "string",
      "required": true
    },
		"en_description": {
			"maxLength": 200,
			"type": "text",
			"required": true
		},
    "en_content": {
      "type": "text",
      "required": true
    },
    "zh_title": {
      "type": "string"
    },
		"zh_description": {
			"maxLength": 200,
			"type": "text"
		},
    "zh_content": {
      "type": "text"
    }
  }
}
