{
  "connection": "default",
  "collectionName": "documents",
  "info": {
    "name": "documents",
    "description": "Publicly viewable documents that you can upload with documentation."
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "comment": ""
  },
  "attributes": {
