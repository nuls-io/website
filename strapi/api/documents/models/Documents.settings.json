      "plugin": "upload"
    },
    "en_title": {
      "type": "string",
      "required": true
    },
		"en_content": {
			"type": "text",
			"required": true
		},
    "zh_title": {
      "type": "string"
    },
    "zh_content": {
      "type": "text"
    }
  }
}
